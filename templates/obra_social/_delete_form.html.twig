<form method="post" action="{{ path('obra_social_delete', {'id': obra_social.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ obra_social.id) }}">
    <button class="btn">Delete</button>
</form>
