{% extends 'base.html.twig' %}

{% block title %}Nuevo Paciente{% endblock %}
{% block panelTitle %}Nuevo Paciente{% endblock %}
{% block body %}

    {{ include('cliente/_form.html.twig') }}

{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('assets/js/cliente.js') }}"></script>
    <script>
        {% if error is not empty %}
            alert("{{ error }}");
            var oldHabitacion = document.getElementById("cliente_habitacion"); 
            $('#cliente_habitacion').val(oldHabitacion.value).trigger('change');
        {% endif %}
    </script>
{% endblock %}